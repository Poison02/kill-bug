        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.killbug.chat.mapper.PrivateMessageMapper">

    <resultMap type="com.killbug.chat.api.domain.PrivateMessage" id="PrivateMessageResult">
